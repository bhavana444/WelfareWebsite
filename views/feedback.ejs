
<%- include("partials/header"); -%>

    <h1>Feedbacks</h1>
    
    <form class="" action="/publish" method="post">
      <button class="btn btn-primary" type="submit" name="button">Compose</button><br/>
      <br/>
    </form>
  <%  posts.forEach(function(post){ %>

    <h2><%=post.title%></h2>
    <p>
    <%= post.content.substring(0,100)+ " ..." %>
    <a href="/posts/<%=post._id%>">Read More</a>
    </p>


    <% }) %>
    

<%- include("partials/footer"); -%>
